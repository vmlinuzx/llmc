{
  "serverConfig": {
    "command": "/bin/sh",
    "args": [
      "-c"
    ]
  },
  "mcpServers": {
    "_disabled_desktop-commander": {
      "command": "npx",
      "args": [
        "@wonderwhy-er/desktop-commander@latest"
      ]
    },
    "llmc": {
      "command": "/home/vmlinux/src/llmc/.venv/bin/python",
      "args": [
        "-m",
        "llmc_mcp.server"
      ],
      "env": {
        "PYTHONPATH": "/home/vmlinux/src/llmc",
        "LLMC_ROOT": "/home/vmlinux/src/llmc",
        "LLMC_CONFIG": "/home/vmlinux/src/llmc/llmc.toml"
      }
    }
  }
}
